package examples

env: {
    TEST_VAR: "test"
}

tasks: {
    "fail-test": {
        description: "Test task that fails with output"
        command: """
            echo "Starting task..."
            echo "This is stdout line 1"
            echo "This is stdout line 2"
            echo "Error: Something went wrong!" >&2
            echo "This is stderr line 1" >&2
            echo "This is stderr line 2" >&2
            echo "Final stdout message"
            exit 1
        """
    }
}
